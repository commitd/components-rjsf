{
  "version": "0.1.0-beta.1",
  "name": "@committed/components-rjsf",
  "description": "Committed Components React Json Schema Forms Library",
  "author": "Committed",
  "license": "MIT",
  "private": false,
  "repository": "git://github.com/commitd/components-rjsf.git",
  "main": "dist/committed-components-rjsf.cjs.js",
  "module": "dist/committed-components-rjsf.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "postinstall": "rimraf node_modules/@emotion/core/types",
    "build": "rollpkg build --tsconfig ./tsconfig.build.json",
    "watch": "rollpkg watch",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:ci": "jest --coverage",
    "test:watch": "jest --watchAll",
    "coverage": "npx live-server coverage/lcov-report",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier --write 'src/**/{*.js,*.ts,*.tsx,*.json,*.md,*.mdx}'",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "size-limit": [
    {
      "path": "dist/components-rjsf.cjs.production.min.js",
      "limit": "40 KB"
    },
    {
      "path": "dist/components-rjsf.esm.js",
      "limit": "40 KB"
    }
  ],
  "eslintConfig": {
    "extends": [
      "./node_modules/rollpkg/configs/eslint"
    ],
    "plugins": [
      "import"
    ],
    "parser": "@typescript-eslint/parser",
    "settings": {
      "import/parsers": {
        "@typescript-eslint/parser": [
          ".ts",
          ".tsx"
        ]
      },
      "import/resolver": {
        "typescript": {
          "alwaysTryTypes": true
        }
      }
    },
    "rules": {
      "import/default": "off",
      "import/no-default-export": "off",
      "react/display-name": "off",
      "import/no-named-as-default-member": "warn"
    },
    "ignorePatterns": [
      "dist",
      "node_modules",
      "docs",
      "**/*.stories.tsx",
      "**/*.test.ts"
    ]
  },
  "jest": {
    "preset": "rollpkg",
    "setupFiles": [
      "./jest.patch-cssom.js"
    ],
    "snapshotSerializers": [
      "jest-stitches"
    ],
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleDirectories": [
      "node_modules",
      "utils"
    ],
    "testRegex": "/src/.*(\\.|/)(test|spec)\\.[jt]sx?$",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/types/**/*"
    ],
    "coveragePathIgnorePatterns": [
      "/src/typings.d.ts",
      "setupTests.tsx",
      ".*/index.ts",
      ".*.test.ts",
      ".*.stories.tsx"
    ]
  },
  "devDependencies": {
    "@fontsource/dosis": "^4.3.0",
    "@fontsource/inter": "^4.3.0",
    "@sinclair/typebox": "^0.16.7",
    "@size-limit/preset-small-lib": "^4.6.2",
    "@storybook/addon-docs": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "@storybook/storybook-deployer": "^2.8.7",
    "@storybook/theming": "^6.2.8",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "jest-stitches": "^1.0.4",
    "jest-watch-typeahead": "^0.6.3",
    "pretty-quick": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "resize-observer-polyfill": "^1.5.1",
    "rollpkg": "^0.5.6",
    "size-limit": "^4.10.2",
    "story-description-loader": "^1.0.0",
    "storybook-dark-mode": "^1.0.7",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "@committed/components": "^5.0.0-beta.6",
    "@mdi/js": "^5.9.55",
    "@rjsf/core": "^2.5.1"
  }
}
